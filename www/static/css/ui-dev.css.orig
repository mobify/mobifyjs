/* Home Page Variation: Display Video */
.animate-video .video-intro {
	position: absolute;
	opacity: 0;
	-webkit-transition: opacity 0.5s ease-out;
}
.animate-video .video-container {
	width: 100%;
	margin-left: 0;
	text-align: center;	
	-webkit-transition: width 0.5s ease-out;
}
.animate-video .video {
	padding: 10px;
	height: 300px;
	background: #272822;
	-webkit-transition: height 0.5s ease-out;
}
.animate-video .video .play {
	display: none;
}
.animate-video .video h3 {
	position: absolute;
	left: 0;
	bottom: 0;
	width: 100%;
	opacity: 0;
	-webkit-transition: opacity 0.5s ease-out;
}
.animate-video iframe {
	display: inline-block;
}
.display-video .video h3 {
	display: none;
}
@media (min-width: 768px) {
	.animate-video .video {
		padding: 10px;
		height: 580px;
	}	
}


/* Page Layout: Docs */
.docs .span8 {
	width: 62%;
	padding-left: 3.95744680199999%;
	float: right;
	margin-bottom: 50px;
}
.docs .span4 {
	float: left;
	margin-left: 0;
}
.docs-nav {
	background: rgba(255,255,255,0.15);
	padding: 20px 40px;
}
	.docs-nav h1 {
		display: none;
	}
	.docs-nav a, .docs-nav a:visited {
		color: #fff;
	}
	.docs-nav ul {
		margin: 0;
	}
	.docs-nav li {
		list-style: none;
		margin: 1em 0;
		line-height: 1.5em;
	}
		.docs-nav li li {
			margin: 0.5em 0;
			font-size: 0.833333em;
		}
		.docs-nav li li a, .docs-nav li li a:visited {
			color: #f29a5c;
		}
		.docs-nav li li code {
			color: #f29a5c;
			background: none;
			padding: 0;
			font-size: 0.888889em;
			-webkit-box-shadow: none;
			-moz-box-shadow: none;
			box-shadow: none;
		}
		.docs-nav li li li {
			display: none;
		}
#markdown-toc {
	margin: 1em 0 2em 0;
}
#markdown-toc li {
	list-style: none;
	margin: 1.5em 0 0 0;
}
	#markdown-toc li code {
		color: #f29a5c;
		background: none;
		padding: 0;
		font-size: 0.888889em;
		-webkit-box-shadow: none;
		-moz-box-shadow: none;
		box-shadow: none;
	}
	#markdown-toc li ul {
		margin: 0;
	}
	#markdown-toc li li {
		margin: 0.5em 0 0 1.5em;
		list-style: decimal;
	}
	#markdown-toc li li li {
		list-style: square;
	}
@media (max-width: 767px) {
	.docs .span8 {
		padding: 0;
		width: 100%;
	}
	.docs .span4 {
		width: 100%;
	}
	.docs-nav li li {
		font-size: 1em;
	}
	.docs-nav li li code {
		font-size: 1em;
	}
}



